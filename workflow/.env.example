# Workflow Automation Configuration
# Copy this file to .env and update with your values

# n8n Configuration
N8N_USER=admin
N8N_PASSWORD=your_secure_password_here
N8N_HOST=localhost
N8N_ENCRYPTION_KEY=your_encryption_key_here

# PostgreSQL Configuration
POSTGRES_USER=freight
POSTGRES_PASSWORD=your_postgres_password_here

# Redis Configuration
REDIS_PASSWORD=your_redis_password_here

# Dify Configuration
DIFY_SECRET_KEY=your_dify_secret_key_here

# API Keys (for integrations)
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
HERE_TRAFFIC_API_KEY=your_here_api_key_here
AVIATIONSTACK_API_KEY=your_aviationstack_api_key_here
TERMINAL49_API_KEY=your_terminal49_api_key_here

# Freight App Integration
FREIGHT_APP_URL=https://freightracing.ca
FREIGHT_APP_WEBHOOK_SECRET=your_webhook_secret_here
